<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.helpticket">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".mainModel.CreateTicketActivity"
            android:label="@string/title_activity_create_ticket"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".mainModel.ReportActivity"
            android:label="@string/title_activity_report"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".mainModel.DetailsActivity"
            android:label="@string/title_activity_details"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".mainModel.AllTicketActivity"
            android:label="@string/title_activity_all_ticket"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".mainModel.UnsolvedTicketActivity"
            android:label="@string/title_activity_unsolved_ticket"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".mainModel.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mainModel.SolvedTicketActivity" />
    </application>

</manifest>